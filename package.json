{
  "name": "ugmk_test_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.2.0",
    "json-server": "^0.17.3",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.3",
    "objectmodel": "^4.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "server": "json-server --watch products.json --port 3001",
    "client": "PORT=3000 react-scripts start",
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "build": "react-scripts build",
    "dockerize": "sudo docker image build -t ugmk_test_app:latest .",
    "start-container": "sudo docker run -p 3000:80 --rm ugmk_test_app"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
